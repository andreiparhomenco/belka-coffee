# ==============================================
# LOCAL DEVELOPMENT MODE
# ==============================================
# 
# Этот файл нужно переименовать в .env.local
# для активации режима разработки
#

# Включить режим разработки с mock данными
VITE_DEV_MODE=true

# Mock пользователь для разработки
# Роль: admin или barista
VITE_MOCK_USER_ROLE=admin
VITE_MOCK_USER_NAME=Тестовый Админ
VITE_MOCK_USER_ID=999999999

